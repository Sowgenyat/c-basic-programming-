#include<stdio.h>
int factors(int n)
{
    int i;
    int sum=0;
    for(i=1;i<n;i++)
    {
        if(n%i==0)
        {
            
            sum=sum+i;
        }  
        
       
    }
   
    if(sum==n)
    {
        printf("1");
    }
    else
    {
        printf("0");
    }
}
    
int main()
{
    factors(6);
    return 0;
}
